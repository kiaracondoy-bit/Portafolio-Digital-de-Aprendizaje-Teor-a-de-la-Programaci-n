Algoritmo sin_titulo
	//Asinganicones
	Definir a Como Entero;
	Definir b Como Real;
	Definir resultado Como Real;

	Escribir 'Ingrese el valor de a';
	Leer a;
	Escribir 'Ingrese el valor de b como decimal';
	Leer b;
	
	resultado = a+b
	Escribir 'El resultado es: ', resultado;
	
FinAlgoritmo
