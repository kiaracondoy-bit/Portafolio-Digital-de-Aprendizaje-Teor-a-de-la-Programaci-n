Algoritmo metrosAPulgadas
	//Declaración de variables
	Definir TMetros Como Real;
	Definir TPulgadas Como Real;
	Definir PULGADA Como Real;
	PULGADA=39.37;
	//Datos de entrada
	Escribir 'Ingrese la cantidad de tela en metros';
	Leer TMetros;
	
	//Proceso 
	TPulgadas= Tmetros*PULGADA;
	
	//Salida
	Escribir 'Por la cantidad de tela expresada en metros hay ', TPulgadas, ' pulgadas';
FinAlgoritmo
